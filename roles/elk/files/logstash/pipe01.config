input {
  beats {
    port => 5044
  }
}

output {
  elasticsearch {
    hosts => ["https://elastic1:9200"]
    ssl_certificate_authorities => ['/usr/share/logstash/config/certs/ca/ca.crt']
    index => "%{[@metadata][beat]}-journald"
    user => "logstash_internal"
    password => "${PROJECT_ELASTIC_LOGSTASH_PASSWORD}"
  }
}
